{
  "name": "northrook/symfony-components-bundle",
  "description": "A component library for Symfony, using Latte.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Northrook\\Symfony\\Components\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.2",
    "northrook/core": "dev-main",
    "northrook/types": "dev-main",
    "northrook/logger": "dev-main",
    "northrook/elements": "dev-main",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/deprecation-contracts": "^3.0",
    "symfony/http-kernel": "^7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "northrook/symfony-core-bundle": "dev-main",
    "northrook/symfony-latte-bundle": "dev-main",
    "northrook/symfony-stylesheets-bundle": "dev-main"
  }
}